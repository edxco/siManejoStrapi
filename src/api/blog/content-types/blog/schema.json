{
  "kind": "collectionType",
  "collectionName": "blogs",
  "info": {
    "singularName": "blog",
    "pluralName": "blogs",
    "displayName": "blog",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "titulo": {
      "type": "string"
    },
    "contenido": {
      "type": "customField",
      "options": {
        "preset": "toolbar"
      },
      "customField": "plugin::ckeditor5.CKEditor"
    },
    "resumen": {
      "type": "text"
    },
    "media": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "videos"
      ]
    },
    "blog_categoria": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::blog-categoria.blog-categoria",
      "inversedBy": "blog"
    },
    "slug": {
      "type": "uid",
      "targetField": "titulo"
    }
  }
}
